<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="-6dp" android:top="-6dp" android:right="-6dp" android:bottom="-6dp">
        <shape><corners android:radius="18dp"/><gradient
            android:type="radial" android:centerX="50%" android:centerY="45%"
            android:gradientRadius="72dp"
            android:startColor="#33000000" android:centerColor="#1A000000" android:endColor="#00000000"/></shape>
    </item>
    <item>
        <shape><corners android:radius="18dp"/><solid android:color="#9E9D24"/></shape>
    </item>
    <item android:top="1dp" android:left="1dp" android:right="1dp">
        <shape><corners android:radius="18dp"/><stroke android:width="1dp" android:color="#40FFFFFF"/></shape>
    </item>
    <item>
        <shape><corners android:radius="18dp"/><gradient
            android:type="linear" android:angle="90"
            android:startColor="#00000000" android:centerColor="#12000000" android:endColor="#25000000"/></shape>
    </item>
</layer-list>
